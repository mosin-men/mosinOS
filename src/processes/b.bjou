# b.bjou
#
# process b

module process_b

using import "console.bjou"
using import "asm.bjou"

type b {
    const STACK_SIZE := 512
    const QM         := 1
    const PROC_NAME  := "b"

    proc routine() {
        i := 0
        while true {
            write("b: ")  writeln(i)
            i -= 1
            asm$wfi()
        }
    }
}

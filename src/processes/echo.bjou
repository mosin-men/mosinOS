# echo.bjou

module process_echo

using import "console.bjou"
using import "asm.bjou"
using import "brkpt.bjou"

type echo {
    const STACK_SIZE := 1024
    const QM         := 1
    const PROC_NAME  := "echo"

    proc routine(data : void*, data_len : u32) {
        writeln(data as char*)
    }
}

# c.bjou
#
# process c

module process_c

using import "console.bjou"
using import "asm.bjou"

type c {
    const STACK_SIZE := 512
    const QM         := 1
    const PROC_NAME  := "c"

    proc routine() {
        i := 1
        while true {
            if i == 0
                i = 1
            write("c: ")  writeln(i)
            i += i
            asm$wfi()
        }
    }
}

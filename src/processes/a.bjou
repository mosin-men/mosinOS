# process a

module process_a

using import "console.bjou"
using import "asm.bjou"

type a {
    const STACK_SIZE := 512
    const QM         := 1
    const PROC_NAME  := "a"

    proc routine() {
        i := 0
        while true {
            write("a: ")  writeln(i)
            i += 1
            asm$wfi()
        }
    }
}
